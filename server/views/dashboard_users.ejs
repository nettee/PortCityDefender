          <h2 class="sub-header">用户列表</h2>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>姓名</th>
                  <th>级别</th>
                  <th>区域</th>
                  <th>描述</th>
                  <th>电话</th>
                </tr>
              </thead>
              <tbody>
                <% for (var i = 0; i < users.length; i++) { %>
                  <tr>
                    <td><%= users[i].id %></td>
                    <td><%= users[i].name %></td>
                    <td><%= users[i].level %></td>
                    <td><%= users[i].region %></td>
                    <td><%= users[i].description %></td>
                    <td><%= users[i].phone %></td>
